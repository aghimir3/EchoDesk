# OpenAI API Key for accessing OpenAI services
OPENAI_API_KEY=your_openai_api_key_here

# Freshdesk domain (e.g., yourcompany.freshdesk.com)
FRESHDESK_DOMAIN=your_freshdesk_domain_here

# Freshdesk API Key for authentication
FRESHDESK_API_KEY=your_freshdesk_api_key_here

# Azure AD Credentials
AZURE_TENANT_ID=your_azure_tenant_id_here
AZURE_CLIENT_ID=your_azure_client_id_here
AZURE_CLIENT_SECRET=your_azure_client_secret_here

# Database connection string
# Example: mssql+pyodbc://username:password@server:port/database?driver=ODBC+Driver+17+for+SQL+Server
DATABASE_URL=your_database_url_here

# Default password for new users (optional)
DEFAULT_PASSWORD=your_default_password_here